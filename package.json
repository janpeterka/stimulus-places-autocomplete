    "@hotwired/stimulus": "^3.0.1",
    "@testing-library/dom": "^8.13.0",
    "@types/google.maps": "^3.48.7",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "mutationobserver-shim": "^0.3.7",
    "np": "^7.6.1",
    "prettier-standard": "16.4.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.7"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.1"
  }
