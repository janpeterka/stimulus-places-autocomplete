{
  "name": "stimulus-places-autocomplete",
  "version": "0.5.0",
  "description": "A Stimulus controller for Google Places Autocomplete.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "google maps",
    "google places autocomplete"
  ],
  "repository": "git@github.com:stimulus-components/stimulus-places-autocomplete.git",
  "bugs": {
    "url": "https://github.com/stimulus-components/stimulus-places-autocomplete/issues"
  },
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/stimulus-components/stimulus-places-autocomplete",
  "private": false,
  "main": "dist/stimulus-places-autocomplete.umd.js",
  "module": "dist/stimulus-places-autocomplete.es.js",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
